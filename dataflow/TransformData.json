{
	"name": "TransformData",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RealtorParquetInput0",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataForSale",
						"type": "DatasetReference"
					},
					"name": "forSaleOutput"
				},
				{
					"dataset": {
						"referenceName": "RealtorReadyToBuild",
						"type": "DatasetReference"
					},
					"name": "ReadyToBuildOutput"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Prop_0 as string,",
				"          Prop_1 as string,",
				"          Prop_2 as string,",
				"          Prop_3 as string,",
				"          Prop_4 as string,",
				"          Prop_5 as string,",
				"          Prop_6 as string,",
				"          Prop_7 as string,",
				"          Prop_8 as string,",
				"          Prop_9 as string,",
				"          Prop_10 as string,",
				"          Prop_11 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> source1",
				"source1 select(mapColumn(",
				"          status = Prop_0,",
				"          price = Prop_1,",
				"          bed = Prop_2,",
				"          bath = Prop_3,",
				"          acre_lot = Prop_4,",
				"          full_address = Prop_5,",
				"          street = Prop_6,",
				"          city = Prop_7,",
				"          state = Prop_8,",
				"          zip_code = Prop_9,",
				"          house_size = Prop_10,",
				"          sold_date = Prop_11",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 split(status=='for_sale',",
				"     status=='ready_to_build',",
				"     disjoint: false) ~> split1@(dataForSale, ReadyToBuild, all)",
				"split1@dataForSale sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> forSaleOutput",
				"split1@ReadyToBuild sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ReadyToBuildOutput"
			]
		}
	}
}