{
	"name": "aggregateTest",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "ASATable"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "FareAverage",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "aggregateFare"
				}
			],
			"scriptLines": [
				"source(output(",
				"          tripID as string,",
				"          createdAt as timestamp,",
				"          startLocation as string,",
				"          endLocation as string,",
				"          distance as integer,",
				"          FARE as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> ASATable",
				"ASATable aggregate(FARE = avg(toInteger(FARE))) ~> aggregateFare",
				"ASATable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}